{
  "name": "launch-web",
  "version": "1.0",
  "description": "I'm sure this is a feature",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "images/icon.png"
    }
  },
  "icons": {
    "48": "images/icon.png"
  },
  "web_accessible_resources": [
    "payload.js",
    "fonts/Folks-Bold.ttf"
  ],
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "manifest_version": 2
}